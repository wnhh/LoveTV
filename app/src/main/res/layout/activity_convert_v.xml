<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/activity_convert_v"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background"
    tools:context="com.yztc.lovetv.activity.ConvertVActivity"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.v7.widget.Toolbar
        android:id="@+id/tb_convert"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/colorWhite"
        android:title="个人资料"
        app:navigationIcon="@mipmap/live_hor_back_normal"
        >

        <TextView
            android:id="@+id/convert_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="绑定邮箱"
            android:textColor="#000000"
            android:textSize="18sp"
            />
    </android.support.v7.widget.Toolbar>
    <FrameLayout
        android:id="@+id/convert_fl"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </FrameLayout>

</LinearLayout>
